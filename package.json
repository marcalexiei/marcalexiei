{
  "name": "marcalexiei",
  "version": "1.0.0",
  "description": "Github profile page",
  "engines": {
    "node": ">= 20"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
  "scripts": {
    "prepare": "husky",
    "format": "biome format .",
    "format:fix": "npm run format -- --write",
    "lint": "markdownlint ./*.md",
    "lint:fix": "npm run lint -- --fix",
    "spellcheck": "npx cspell . --show-context",
    "test": "npm run spellcheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcalexiei/marcalexiei.git"
  },
  "author": "Marco Pasqualetti <marco.pasqualetti@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcalexiei/marcalexiei/issues"
  },
  "homepage": "https://github.com/marcalexiei/marcalexiei#readme",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "cspell": "8.14.2",
    "husky": "9.1.5",
    "markdownlint": "0.35.0",
    "markdownlint-cli": "0.41.0"
  }
}
