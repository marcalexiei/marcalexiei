{
  "name": "marcalexiei",
  "version": "1.0.0",
  "description": "Github profile page",
  "engines": {
    "node": ">= 20"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
  "scripts": {
    "prepare": "husky",
    "format": "biome format .",
    "format:fix": "npm run format -- --write",
    "lint": "markdownlint ./*.md",
    "lint:fix": "npm run lint -- --fix",
    "spellcheck": "npx cspell . --show-context",
    "test": "npm run spellcheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcalexiei/marcalexiei.git"
  },
  "author": "Marco Pasqualetti <marco.pasqualetti@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcalexiei/marcalexiei/issues"
  },
  "homepage": "https://github.com/marcalexiei/marcalexiei#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "cspell": "8.14.4",
    "husky": "9.1.6",
    "markdownlint": "0.35.0",
    "markdownlint-cli": "0.42.0"
  }
}
