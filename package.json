  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcalexiei/marcalexiei/issues"
  },
  "homepage": "https://github.com/marcalexiei/marcalexiei#readme",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "cspell": "8.13.3",
    "husky": "9.1.4",
    "markdownlint": "0.34.0",
    "markdownlint-cli": "0.41.0"
  }
}
